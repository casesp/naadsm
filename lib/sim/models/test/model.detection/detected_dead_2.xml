<?xml version="1.0" encoding="UTF-8"?>
<naadsm:disease-simulation
  xmlns:naadsm="http://www.naadsm.org/schema"
  xmlns:xdf="http://xml.gsfc.nasa.gov/XDF">
  <description>Specs for a sample simulation run.</description>
  <num-days>7</num-days>
  <num-runs>1</num-runs>

  <models>

    <zone-model>
      <name>High risk</name>
      <level>1</level>
      <radius>
        <value>15</value>
        <units><xdf:unit>km</xdf:unit></units>
      </radius>
    </zone-model>

    <zone-model>
      <name>Background</name>
      <level>2</level>
      <radius>
        <value>0</value>
        <units><xdf:unit>km</xdf:unit></units>
      </radius>
    </zone-model>

    <disease-model production-type="Beef Cattle">
      <latent-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </latent-period>
      <infectious-subclinical-period>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-subclinical-period>
      <infectious-clinical-period>
        <point>0</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-clinical-period>
      <mortality>1</mortality>
      <immunity-period>
        <point>3</point>
        <units><xdf:unit>day</xdf:unit></units>
      </immunity-period>
    </disease-model>

    <disease-model production-type="Pigs">
      <latent-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </latent-period>
      <infectious-subclinical-period>
        <point>1</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-subclinical-period>
      <infectious-clinical-period>
        <point>20</point>
        <units><xdf:unit>day</xdf:unit></units>
      </infectious-clinical-period>
      <immunity-period>
        <point>3</point>
        <units><xdf:unit>day</xdf:unit></units>
      </immunity-period>
    </disease-model>

    <!-- With all herds size=25.  The parameters below give a 0.505
      probability of infection for herds 20 km apart. -->
    <airborne-spread-model from-production-type="Pigs" to-production-type="Beef Cattle">
      <herd-size-infectious>25</herd-size-infectious>
      <herd-size-susceptible>25</herd-size-susceptible>
      <herd-distance>
	<value>20</value>
	<units>
	  <xdf:unit>km</xdf:unit>
	</units>
      </herd-distance>
      <prob>0.505</prob>
      <wind-direction-start>
        <value>0</value>
        <units><xdf:unit>degree</xdf:unit></units>
      </wind-direction-start>
      <wind-direction-end>
        <value>360</value>
        <units><xdf:unit>degree</xdf:unit></units>
      </wind-direction-end>
    </airborne-spread-model>

    <!-- The probability of detection vs. days clinical for cattle is
      below 0.5 until a day has passed.  So a cattle unit outside of
      any zone will be detected the day after it starts showing
      clinical signs. -->
    <detection-model production-type="Beef Cattle">
      <prob-detect-vs-time-clinical>
        <value>0</value> <value>0</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </prob-detect-vs-time-clinical>
      <prob-detect-vs-time-dead>
        <value>0</value> <value>0.4</value>
        <value>1</value> <value>0.6</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </prob-detect-vs-time-dead>
      <prob-report-clinical-signs-vs-time-since-outbreak>
        <value>0</value> <value>1.0</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unitless /></units>
      </prob-report-clinical-signs-vs-time-since-outbreak>
    </detection-model>

    <resources-and-implementation-of-controls-model>
      <!-- The authorities can begin a destruction program immediately. -->
      <destruction-program-delay>
        <value>0</value>
        <units><xdf:unit>day</xdf:unit></units>	
      </destruction-program-delay>
      <!-- No destruction capacity. -->
      <destruction-capacity>
        <value>0</value> <value>0</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unit>herd</xdf:unit><xdf:unit power="-1">day</xdf:unit></units>
      </destruction-capacity>
      <destruction-priority-order>
        reason,production type,time waiting
      </destruction-priority-order>

      <!-- Vaccination isn't used in this scenario. -->
      <vaccination-program-delay>0</vaccination-program-delay>
      <vaccination-capacity>
        <value>0</value> <value>0</value>
        <units><xdf:unit>day</xdf:unit></units>
        <units><xdf:unit>herd</xdf:unit><xdf:unit power="-1">day</xdf:unit></units>
      </vaccination-capacity>
      <vaccination-priority-order>
        reason,production type,time waiting
      </vaccination-priority-order>
    </resources-and-implementation-of-controls-model>

    <detection-monitor>
      <output>
        <variable-name>detnU</variable-name>
        <frequency>daily</frequency>
      </output>
    </detection-monitor>

    <conflict-resolver></conflict-resolver>

    <full-table-writer />

  </models>

</naadsm:disease-simulation>
